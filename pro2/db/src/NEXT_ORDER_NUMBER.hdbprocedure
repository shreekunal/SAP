CREATE PROCEDURE "NEXT_ORDER_NUMBER" (
   OUT ORDER_NUMBER NVARCHAR(10)
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
AS
BEGIN
   DECLARE seq_value BIGINT;

   -- Get next value from HANA sequence
   SELECT NEXT VALUE FOR "DEMO"
     INTO seq_value
     FROM DUMMY;

   -- Format as ORD-00001, ORD-00002, etc.
   ORDER_NUMBER := 'ORD-' || LPAD(TO_NVARCHAR(seq_value), 5, '0');
END;
