FUNCTION "GET_NEXT_ORDER_NUMBER"()
RETURNS TABLE (
   ORDER_NUMBER NVARCHAR(10)
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
READS SQL DATA
AS
BEGIN
   DECLARE LV_SEQ_VALUE INTEGER;
   
   SELECT NEXT VALUE FOR "DEMO" INTO LV_SEQ_VALUE FROM DUMMY;
   
   RETURN SELECT 'ORD-' || LPAD(CAST(LV_SEQ_VALUE AS NVARCHAR(5)), 5, '0') as ORDER_NUMBER FROM DUMMY;
END
