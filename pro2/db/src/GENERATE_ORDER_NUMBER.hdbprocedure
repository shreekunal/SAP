PROCEDURE "GENERATE_ORDER_NUMBER" (
   OUT OV_ORDER_NUMBER NVARCHAR(10)
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER
AS
BEGIN
   DECLARE LV_SEQ_VALUE INTEGER;
   
   SELECT NEXT VALUE FOR "DEMO" INTO LV_SEQ_VALUE FROM DUMMY;
   
   OV_ORDER_NUMBER := 'ORD-' || LPAD(CAST(LV_SEQ_VALUE AS NVARCHAR(5)), 5, '0');
END
