{
  "name": "mainCap",
  "version": "1.0.0",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/connectivity": "^4.4.0",
    "@sap-cloud-sdk/http-client": "^4.4.0",
    "@sap-cloud-sdk/resilience": "^4.4.0",
    "@sap/cds": "^9",
    "@sap/xssec": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "private": true,
  "cds": {
    "requires": {
      "OrdersServiceRemote": {
        "kind": "odata",
        "credentials": { "destination": "devpro4" },
        "path": "/odata/v4/orders"
      },
      "EmployeesServiceRemote": {
        "kind": "odata",
        "credentials": { "destination": "devpro5" },
        "path": "/odata/v4/employees"
      }
    },
    "[development]": {
      "requires": {
        "OrdersServiceRemote": {
          "credentials": {
            "destination": null,
            "url": "http://localhost:4004/odata/v4/orders"
          }
        },
        "EmployeesServiceRemote": {
          "credentials": {
            "destination": null,
            "url": "http://localhost:4005/odata/v4/employees"
          }
        }
      }
    },
    "[production]": { "auth": "xsuaa" }
  }
}
