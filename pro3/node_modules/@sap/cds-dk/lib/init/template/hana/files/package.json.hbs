    {{#profile}}
    "[{{profile}}]": {
    {{/profile}}
    {{#shortcut}}
      "db": "hana"
    {{/shortcut}}
    {{^shortcut}}
      "db": {
        "kind": "hana"
      }
    {{/shortcut}}
    {{#profile}}
    }
    {{/profile}}
  }
  {{/isJava}}
}
