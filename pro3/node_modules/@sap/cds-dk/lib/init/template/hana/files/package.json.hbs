{
  {{#isNodejs}}
  "dependencies": {
    "@cap-js/hana": "^2"
  }{{#profile}},
  "cds": {
    "requires": {
      "[{{profile}}]": {
      {{#shortcut}}
        "db": "hana"
      {{/shortcut}}
      {{^shortcut}}
        "db": {
          "kind": "hana"
        }
      {{/shortcut}}
      }
    }
  }
  {{/profile}}
  {{/isNodejs}}
  {{#isJava}}
